{
  "name": "jsonchunk",
  "version": "0.1.0",
  "description": "Parse partial JSON as it streams in. Type-safe, tiny, built for LLM output.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "json",
    "partial",
    "streaming",
    "parser",
    "llm",
    "ai",
    "typescript",
    "type-safe",
    "incremental"
  ],
  "license": "MIT"
}
